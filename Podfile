
platform :ios, '8.0'
inhibit_all_warnings!

def pods

    pod 'UIColor_Hex_Swift'             #设置色值颜色
    pod 'SnapKit'                       #自动布局 它的作者仍然是写Objective-C的第三方库Masonry的大牛
    pod 'MBProgressHUD', '~> 0.9.2'
    pod 'Toast', '~> 3.0'               # 提示窗

end

target 'PRCommonMoudleForSwift' do
    pods
end
